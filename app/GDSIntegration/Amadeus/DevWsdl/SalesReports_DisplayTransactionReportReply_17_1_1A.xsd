<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://xml.amadeus.com/TSRTRR_17_1_1A" xmlns="http://xml.amadeus.com/TSRTRR_17_1_1A" elementFormDefault="qualified">
  <xs:element name="SalesReports_DisplayTransactionReportReply">
    <xs:annotation>
      <xs:documentation xml:lang="en">Transaction sales report query response
Sends back the reporting data related to a transaction
All tha data related to one given transaction are provided to the client.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="errorGroup" minOccurs="0" type="ErrorGroupType">
          <xs:annotation>
            <xs:documentation xml:lang="en">Conveys a potential error and its associated Free flow text.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="transactionReportDataDetails" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="en">Conveys all the details associated to a transaction.</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="agencyDetails" type="AdditionalBusinessSourceInformationTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Conveys agency details:  - IATA number - Office ID.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="stockProviderDetails" minOccurs="0" type="CompanyInformationType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">conveys the Stock Provider details (industry sector and code)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="agentDetails" type="TicketAgentInfoType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Conveys details related to the agent associated to the requested document.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="agentSineInfo" minOccurs="0" type="UserIdentificationType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Conveys details related to the agent or user associated to the requested document.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="agentOtherUserDetails" minOccurs="0" maxOccurs="5" type="AttributeType_209106S">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Conveys details related to the agent or user associated to the requested document such as the agent LSS log in and the agent full name</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="validatingCarrierDetails" minOccurs="0" type="TransportIdentifierType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Conveys the validating carrier details.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="dateDetails" type="StructuredDateTimeInformationType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Conveys the issuance date.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="documentNumber" type="TicketNumberTypeI_51826S">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Logical document number with airline code and related details.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="transaction" type="TransactionInformationForTicketingType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Transaction details:  - transaction code - transaction type</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="documentIdentification" type="ItemNumberTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">This conveys the sequence number of the document within the sales report.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="monetaryInfo" type="MonetaryInformationTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Conveys various document amounts:  - fees amount - commission amount - total taxes amount</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="reservationInformation" minOccurs="0" maxOccurs="9" type="ReservationControlInformationType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Conveys details about document reservation reference (1A PNR or provider reference for rail transactions)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="passengerName" minOccurs="0" type="TravellerInformationTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Conveys the passenger name.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="enhancedPaxInfoCriteria" minOccurs="0" type="EnhancedTravellerInformationType" />
              <xs:element name="tourCodeDetails" minOccurs="0" type="TourInformationTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Conveys the information about the tour code associated to the document.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="statusDetails" minOccurs="0" type="StatusType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">To convey details about document status (refund reason for rail refund transactions)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="invoiceDetails" minOccurs="0" maxOccurs="2" type="DocumentInformationDetailsTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Conveys the invoice details. For specific Rail, can also convey the Rail provider document number</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="fopGroup" maxOccurs="9" type="FOPRepresentationType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">This group conveys data related to the FOP.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="tax" minOccurs="0" maxOccurs="3" type="TaxType" />
              <xs:element name="fareCalcDetails" minOccurs="0" maxOccurs="9" type="CodedAttributeType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Conveys the fare calculation details in all cases.   In specific cases (market/airline specifics) it conveys also: - the revenue attribution number/pseudo IATA number (entered in FZRA) - the payment on demand number generated at issuance time - the Cost Center</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="pricingIndicator" minOccurs="0" type="PricingTicketingDetailsTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Conveys the pricing indicator.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="referenceData" minOccurs="0" type="ReferenceInfoType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Conveys the Settlement and authorisation code if present.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="feeGroup" minOccurs="0">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Conveys the fees data if present</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="feeType" type="SelectionDetailsTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Conveys the fee type.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="feeDetails" maxOccurs="99">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Conveys details on the fee:  - fee subcode - fee amount - tax on fee code - tax on fee amount</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="feeSubType" type="SpecificDataInformationTypeI">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Fee subtype</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="feeMonetaryInfo" type="MonetaryInformationTypeI_52231S">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">fee monetary Info</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="feeTaxInfo" minOccurs="0" maxOccurs="99" type="TaxType" />
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="exchangedGroup" minOccurs="0" maxOccurs="99">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Details about exchanged tickets</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="ticketExchanged" type="TicketNumberTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">exchanged Ticket Number</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="textInformation" minOccurs="0" type="InteractiveFreeTextType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Original issue information</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="couponAssociated" minOccurs="0" type="CouponInformationType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Coupon numbers and SAC number</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:complexType name="AdditionalBusinessSourceInformationTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify additional originator and source information.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="sourceType" type="SourceTypeDetailsTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">Source type</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="originatorDetails" type="OriginatorIdentificationDetailsTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">details of the sales report originator.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ApplicationErrorDetailType">
    <xs:sequence>
      <xs:element name="errorCode" type="AlphaNumericString_Length1To5">
        <xs:annotation>
          <xs:documentation xml:lang="en">Code identifying the data validation error condition.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="errorCategory" minOccurs="0" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Identification of a code list.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="errorCodeOwner" minOccurs="0" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Code identifying the agency responsible for a code list.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ApplicationErrorInformationType">
    <xs:annotation>
      <xs:documentation xml:lang="en">To identify the type of application error within a message.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="errorDetails" type="ApplicationErrorDetailType">
        <xs:annotation>
          <xs:documentation xml:lang="en">Application error details.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AttributeInformationTypeU">
    <xs:annotation>
      <xs:documentation xml:lang="en">To identify the type of attribute and the attribute</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="attributeType" type="AlphaNumericString_Length1To25">
        <xs:annotation>
          <xs:documentation xml:lang="en">This is the data type.  may contain:  - CPY   : DCC Provider Identifier Name  - DCC   : DCC Currency  - AMTCC : DCC Amount  - RAT   : DCC Rate  - MGN   : DCC margin percentage</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="attributeDescription" minOccurs="0" type="AlphaNumericString_Length1To256">
        <xs:annotation>
          <xs:documentation xml:lang="en">This is the data value.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AttributeInformationTypeU_289284C">
    <xs:annotation>
      <xs:documentation xml:lang="en">To identify the type of attribute and the attribute</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="attributeType" type="AlphaNumericString_Length1To99" />
      <xs:element name="attributeDescription" minOccurs="0" type="AlphaNumericString_Length1To256" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AttributeType">
    <xs:annotation>
      <xs:documentation xml:lang="en">Used to have tag value without code list for tag</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="criteriaSetType" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Type of information: - is this a switch? - is this a structured data? ...</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="criteriaDetails" type="AttributeInformationTypeU">
        <xs:annotation>
          <xs:documentation xml:lang="en">Details for the message criteria (name, value).</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AttributeType_209106S">
    <xs:annotation>
      <xs:documentation xml:lang="en">Used to have tag value without code list for tag</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="criteriaSetType" minOccurs="0" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Originator Identification Details such as  SLG : agent short LSS Log in AGN : agent Full Name</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="criteriaDetails" maxOccurs="5" type="AttributeInformationTypeU_289284C">
        <xs:annotation>
          <xs:documentation xml:lang="en">Details for the message criteria (name, value).</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CodedAttributeInformationType">
    <xs:annotation>
      <xs:documentation xml:lang="en">Convey coded key and corresponding value</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="attributeType" type="AlphaNumericString_Length1To5">
        <xs:annotation>
          <xs:documentation xml:lang="en">provides the attribute Type</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="attributeDescription" minOccurs="0" type="AlphaNumericString_Length1To256">
        <xs:annotation>
          <xs:documentation xml:lang="en">provides a description for the attribute</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CodedAttributeType">
    <xs:annotation>
      <xs:documentation xml:lang="en">Used to have tag value without code list for tag</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="attributeDetails" maxOccurs="99" type="CodedAttributeInformationType">
        <xs:annotation>
          <xs:documentation xml:lang="en">provides details for the Attribute</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Dummy.NET" minOccurs="0" maxOccurs="0" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CompanyIdentificationTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">Code or name to identify a company and any associated companies.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="marketingCompany" minOccurs="0" type="AlphaNumericString_Length1To35">
        <xs:annotation>
          <xs:documentation xml:lang="en">Validating carrier code on 2 alpha characters.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CompanyInformationType">
    <xs:annotation>
      <xs:documentation xml:lang="en">This segment is used to convey the company information</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="travelSector" minOccurs="0" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">This data element is used to qualify the company code, to identify the industry business it belongs.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="companyCodeContext" minOccurs="0" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">This data element is used to convey the context in which the code applies</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="companyCode" minOccurs="0" type="AlphaNumericString_Length1To35">
        <xs:annotation>
          <xs:documentation xml:lang="en">This data element is used to convey the company code of a company</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="companyName" minOccurs="0" type="AlphaNumericString_Length1To35">
        <xs:annotation>
          <xs:documentation xml:lang="en">This data element is used to convey the company name of a company</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="accessLevel" minOccurs="0" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">This data element is used to convey the type of access the non-air company has with Amadeus.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="companyLongCode" minOccurs="0" type="AlphaNumericString_Length1To35">
        <xs:annotation>
          <xs:documentation xml:lang="en">This data element is used to convey the long company code of a company</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="companyNumericCode" minOccurs="0" type="AlphaNumericString_Length1To15">
        <xs:annotation>
          <xs:documentation xml:lang="en">This data element is used to convey the numeric company code</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CouponInformationDetailsType">
